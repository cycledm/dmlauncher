{
  "name": "dmlauncher",
  "productName": "DMLauncher",
  "description": "DMLauncher",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://github.com/cycledm/dmlauncher#readme",
  "repository": "github:cycledm/dmlauncher",
  "license": "UNLICENSED",
  "author": "CycleDM",
  "main": "./out/main/index.js",
  "scripts": {
    "build": "npm run typecheck && npm run generate-routes && npm run dotenvx-run electron-vite build",
    "build:linux": "electron-vite build && electron-builder --linux",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "dev": "concurrently -k -s command-dev -n tsr,dev 'npm:watch-routes' 'npm:dev:renderer'",
    "dev:renderer": "npm run dotenvx-run:dev electron-vite dev",
    "dotenvx-run": "dotenvx run -f .env.production --",
    "dotenvx-run:dev": "dotenvx run -f .env.development --",
    "format": "prettier --write .",
    "generate-routes": "tsr generate && prettier --write src/renderer/src/routes",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --cache .",
    "prepare": "husky",
    "start": "npm run dotenvx-run electron-vite preview",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "watch-routes": "tsr watch"
  },
  "lint-staged": {
    "*": "prettier --write ."
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "axios": "^1.10.0",
    "chokidar": "^4.0.3",
    "dayjs": "^1.11.13",
    "electron-devtools-installer": "^4.0.0",
    "electron-updater": "^6.6.2",
    "pretty-bytes": "^7.0.0",
    "ts-case-convert": "^2.1.0"
  },
  "devDependencies": {
    "@base-ui-components/react": "1.0.0-beta.0",
    "@dotenvx/dotenvx": "^1.44.2",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@floating-ui/react": "^0.27.12",
    "@fontsource-variable/jetbrains-mono": "^5.2.6",
    "@fontsource-variable/noto-sans-sc": "^5.2.6",
    "@fontsource-variable/noto-serif-sc": "^5.2.6",
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/eslint-plugin-query": "^5.78.0",
    "@tanstack/eslint-plugin-router": "^1.121.21",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-query-devtools": "^5.80.7",
    "@tanstack/react-router": "^1.121.21",
    "@tanstack/react-router-devtools": "^1.121.21",
    "@tanstack/router-cli": "^1.121.21",
    "@tanstack/router-plugin": "^1.121.22",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/culori": "^4.0.0",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "culori": "^4.0.1",
    "electron": "^35.5.1",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "html-react-parser": "^5.2.5",
    "husky": "^9.1.7",
    "i18next": "^25.2.1",
    "i18next-resources-to-backend": "^1.2.1",
    "jotai": "^2.12.5",
    "lint-staged": "^16.1.2",
    "mac-scrollbar": "^0.13.8",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "prettier-plugin-sort-imports": "^1.8.8",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^15.5.3",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "electron",
      "esbuild"
    ]
  }
}
